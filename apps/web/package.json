{
  "name": "@kaya/web",
  "version": "0.1.2",
  "private": true,
  "description": "Kaya web application",
  "type": "module",
  "scripts": {
    "dev": "rsbuild dev",
    "build": "bun run --cwd ../.. generate-version-info && bun run --cwd ../.. copy-assets && bun run --cwd ../.. build:packages && rsbuild build",
    "preview": "rsbuild preview",
    "type-check": "tsc --build --force",
    "lint": "eslint src --ext ts,tsx"
  },
  "dependencies": {
    "@dagrejs/dagre": "^2.0.0",
    "@dagrejs/graphlib": "^2.2.4",
    "@kaya/gametree": "workspace:*",
    "@kaya/goboard": "workspace:*",
    "@kaya/sgf": "workspace:*",
    "@kaya/shudan": "workspace:*",
    "@kaya/ui": "workspace:*",
    "coi-serviceworker": "^0.1.7",
    "i18next": "^25.1.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.1.14",
    "@rsbuild/plugin-react": "^1.1.5",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "typescript": "^5.6.0"
  }
}
