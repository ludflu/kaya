{
  "name": "@kaya/ai-engine",
  "version": "0.1.12-dev",
  "description": "AI engine for Go game analysis using KataGo models",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./tauri-engine": {
      "types": "./dist/tauri-engine.d.ts",
      "import": "./dist/tauri-engine.js",
      "require": "./dist/tauri-engine.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "dev": "tsc --watch",
    "analyze": "bun run src/cli.ts",
    "test": "bun test"
  },
  "keywords": [
    "go",
    "baduk",
    "weiqi",
    "katago",
    "ai",
    "analysis"
  ],
  "dependencies": {
    "@kaya/gametree": "workspace:*",
    "@kaya/goboard": "workspace:*",
    "@kaya/sgf": "workspace:*",
    "onnxruntime-web": "^1.23.2"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.9.3"
  }
}
